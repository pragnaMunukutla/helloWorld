#include <stdio.h>
#include <stdlib.h>

//Remove Duplicates from Array
int removeDuplicates(int* array,int n){
    
    int j=0,i=0;
    
    for(i=0;i<n-1;i++){
        if(array[i]!=array[i+1])
            array[j++]=array[i];
    }
    
    array[j++]=array[n-1];
    
    return j;
}

//Given elements in an array find the element which is not repeated
int findOne(int* array,int n){
    
    int i,res=array[0];
    
    for(i=1;i<n;i++)
        res=res^array[i];
        
    return res;
}
int main()
{
    int* array;
    int n,i,j;
    
    scanf("%d",&n);
    
    array=(int*)malloc(n*sizeof(int));
    
    printf("Enter %d elements",n);
    for(i=0;i<n;i++)
        scanf("%d",&array[i]);
    
    printf("%d",findOne);
    j=removeDuplicates(array,i);
    
    for(i=0;i<j;i++)
        printf("%d ",array[i]);
}
